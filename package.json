{
  "name": "aaronmoat-blog",
  "private": true,
  "description": "Aaron's personal blog",
  "version": "0.1.0",
  "author": "Aaron Moat",
  "dependencies": {
    "gatsby": "^2.18.12",
    "gatsby-image": "^2.2.34",
    "gatsby-plugin-feed": "^2.3.23",
    "gatsby-plugin-google-analytics": "^2.1.29",
    "gatsby-plugin-manifest": "^2.2.31",
    "gatsby-plugin-offline": "^3.0.27",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-s3": "^0.3.2",
    "gatsby-plugin-sharp": "^2.3.5",
    "gatsby-plugin-typography": "^2.3.18",
    "gatsby-remark-copy-linked-files": "^2.1.31",
    "gatsby-remark-images": "^3.1.35",
    "gatsby-remark-prismjs": "^3.3.25",
    "gatsby-remark-responsive-iframe": "^2.2.28",
    "gatsby-remark-smartypants": "^2.1.17",
    "gatsby-source-filesystem": "^2.1.40",
    "gatsby-transformer-remark": "^2.6.39",
    "gatsby-transformer-sharp": "^2.3.7",
    "now": "^16.7.0",
    "prismjs": "^1.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.75",
    "typography": "^0.16.19",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "babel-loader": "^8.0.6",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.19.1",
    "react-vis": "^1.11.7",
    "styled-components": "^4.4.1"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby-starter-blog#readme",
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build && yarn build-storybook -o public/demos -s demos/",
    "develop": "gatsby develop -p 3333",
    "format": "prettier --write \"**/*.{js,jsx,json,md,css}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "yarn build && now",
    "deploy-prod": "yarn build && now --prod",
    "storybook": "start-storybook -p 6006 -s demos"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{gatsby-*.js,src/**/*.{js,jsx,json,md,css}}": [
      "yarn format",
      "git add"
    ]
  }
}
